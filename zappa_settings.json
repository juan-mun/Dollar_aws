{
    "dev": {
        "app_function": "app.lambda_handler",
        "aws_region": "us-east-1",
        "exclude": [
            "boto3",
            "dateutil",
            "botocore",
            "s3transfer",
            "concurrent"
        ],
        "project_name": "parcial-i",
        "runtime": "python3.10",
        "s3_bucket": "dolar-raw-cmjm",
        "keep_warm": false,
        "apigateway_enabled": false,
        "manage_roles": false,
        "role_name": "LabRole",
        "events": [
            {
                "function": "app.lambda_handler",
                "expression": "cron(0 23 * * ? *)"
            }
        ]
    }
}
